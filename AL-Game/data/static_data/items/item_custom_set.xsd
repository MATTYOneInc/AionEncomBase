<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="../modifiers.xsd"/>
	<xs:element name="item_custom_sets">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="item_custom_set" type="ItemCustomSetTemplate" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ItemCustomSetTemplate">
		<xs:attribute name="id" type="xs:int" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="custom_enchant_value" type="xs:int" use="required"/>
	</xs:complexType>
</xs:schema>